<h1>Product Index</h1>
<p></p>

<%= link_to 'New Product', "/products/new" %>

<table>
	<tr>
		<th>Name</th>
		<th>Price</th>
		<th></th>
	</tr>


<% @products.each do |product| %>
	<tr>
		<td><%= link_to product.name, product_path(product) %></td>
<!-- 		<td><%= product.description %></td> -->
		<td><%= product.formatted_price %></td>
	</tr>

<% end %>

</table>


